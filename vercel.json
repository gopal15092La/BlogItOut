{
  "version": 2,
  "builds": [
    {
      "src": "src/views/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "src/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/views/$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "/src/public/$1"
    }
  ]
}
